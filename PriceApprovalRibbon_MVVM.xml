<?xml version="1.0" standalone="yes"?>
<!--Custom Ribbon for Custom Tools -->
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="CustomRibbon.MVVMPriceApprovalRibbon">
