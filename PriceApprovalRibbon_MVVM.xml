               <button id="ButtonA_01" getLabel="MVVMPriceApprovalGetLabel" onAction="MVVMPriceApprovalOnAction" getImage="MVVMPriceApprovalGetImage" getSize="MVVMPriceApprovalGetSize" getScreentip="MVVMPriceApprovalGetScreentip" getVisible="MVVMPriceApprovalGetVisible" tag="MVVM_PriceApproval"/>
               <separator id="MVVM_PriceApprovalGroupASeperator_01"/>
               <labelControl id="LabelA_01" getLabel="MVVMPriceApprovalGetLabel" getScreentip="MVVMPriceApprovalGetScreentip" getVisible="MVVMPriceApprovalGetVisible"/>
               <button id="ButtonA_02" getLabel="MVVMPriceApprovalGetLabel" onAction="MVVMPriceApprovalOnAction" getImage="MVVMPriceApprovalGetImage" getSize="MVVMPriceApprovalGetSize" getScreentip="MVVMPriceApprovalGetScreentip" getVisible="MVVMPriceApprovalGetVisible" tag="MVVM_PriceApproval"/>
            </group>
            <group id="MVVM_PriceApprovalGroupB" getVisible="MVVMPriceApprovalGetVisible" getLabel="MVVMPriceApprovalGetLabel">
               <button id="ButtonB_01" getLabel="MVVMPriceApprovalGetLabel" onAction="MVVMPriceApprovalOnAction" getImage="MVVMPriceApprovalGetImage" getSize="MVVMPriceApprovalGetSize" getScreentip="MVVMPriceApprovalGetScreentip" getVisible="MVVMPriceApprovalGetVisible" tag="MVVM_PriceApproval"/>
               <separator id="MVVM_PriceApprovalGroupBSeperator_01"/>
               <button id="ButtonB_02" getLabel="MVVMPriceApprovalGetLabel" onAction="MVVMPriceApprovalOnAction" getImage="MVVMPriceApprovalGetImage" getSize="MVVMPriceApprovalGetSize" getScreentip="MVVMPriceApprovalGetScreentip" getVisible="MVVMPriceApprovalGetVisible" tag="MVVM_PriceApproval"/>
            </group>
         </tab>
      </tabs>
   </ribbon>
</customUI>
